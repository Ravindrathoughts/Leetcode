class Solution {
    public boolean isPalindrome(String s) {

        String s2 = "";
        for(int i=s.length()-1;i>=0;i--)
        {
           s2  = s2+s.charAt(i);
        }
        if(s==s2)
        {
            return true;
        }
        else{
            return false;
        }
    }

    public static void main(String[] args)
    {
        Solution s=new Solution();
        String s1= "Mom";
        s.isPalindrome(s1);
    }
}
