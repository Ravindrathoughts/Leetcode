class Solution {
    public int addDigits(int num) {
        if(num==0)
        {
            return 0;

        }else{
            return 1+(num-1)%9;

        }

        
    }

    public static void main(String[] args)
    {
        Solution s=new Solution();
        int num1=38;
        int result1=s.addDigits(num1);
        System.out.println("Result" +result1);

    }
}
