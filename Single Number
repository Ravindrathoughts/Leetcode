class Solution {
    public int singleNumber(int[] nums) {
        int result =0;
        for(int num:nums)
        {
            result = result^num;
        }
        return result;
        
    }

    public static void main(String[] args)
    {
        Solution s=new Solution();
        int nums[] = {1,2,2};
        int nums1[] = {1,2,2,3,3};
        System.out.println(s.singleNumber(nums));
    }
}
